{
  "name": "MixYT",
  "version": "1.0",
  "description": "A Chrome extension to allow users to convert a Youtube playlist into a DJ deck.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
